@model IEnumerable<PresentationLayer.Models.PageViewModel>
@{
    ViewBag.Title = "Result";
}

<h2>Result</h2>

<h3 align="center">@ViewBag.HostURL</h3>
<h4 align="center">@ViewBag.Date</h4>

@if (ViewBag.Error != null)
{
    <label>@ViewBag.Error</label>
}

<div class="row">
    <div class="col-md-10 col-md-offset-1" id="tblGrid">
        @Html.Partial("Table", Model)
    </div>
</div>

